@model List<string>
@{
    ViewData["Title"] = "Home Page";

}

<div class="text-center">
    <h1>Alle Namen!</h1>
    @if (Model != null)
    {
        @foreach (string naam in Model)
           {
            <p>@naam</p>
        }
    }
</div>


